
<h1 >Student info</h1>
<hr>
<p><%= @student['first_name'] %></p>
<p><%= @student['last_name'] %></p>
<p><%= @student['email'] %></p>
<p><%= @student['short_bio'] %></p>
<p><%= @student['linkedin_url'] %></p>
<p><%= @student['twitter_handle'] %></p>
<p><%= @student['personal_web_url'] %></p>
<p><%= @student['online_resume_url'] %></p>
<p><%= @student['github_url'] %></p>
<p><%= @student['photo'] %></p>

<h1> Experiences </h1>
<hr>
<p><%= @experience[0]['start_date'] %></p>
<p><%= @experience[0]['end_date'] %></p>
<p><%= @experience[0]['job_title'] %></p>
<p><%= @experience[0]['details'] %></p>

<h1> Education </h1>
<hr>
<p><%= @education[0]['start_date'] %></p>
<p><%= @education[0]['end_date'] %></p>
<p><%= @education[0]['degree'] %></p>
<p><%= @education[0]['university'] %></p>
<p><%= @education[0]['details'] %></p>

<h1> Capstones </h1>
<hr>
<p><%= @capstones[0]['name'] %></p>
<p><%= @capstones[0]['description'] %></p>
<p><%= @capstones[0]['url'] %></p>
<p><img src="<%= @capstones[0]['screenshot'] %>"></p>

<h1> Skills </h1>
<hr>
<% @skills.each do |skill| %>
  <p><%= skill['skill_name'] %></p>
<% end %>


